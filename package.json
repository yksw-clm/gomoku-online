{
  "name": "gomoku-game-server",
  "version": "1.0.0",
  "description": "オンライン五目並べゲームのソケットサーバー",
  "main": "dist/index.js",
  "scripts": {
    "build:client": "cd client && npm install && npm run build",
    "setup:server": "cd server && npm install",
    "build:server": "cd server && npm run build",
    "copy:client": "node copy-client.js",
    "debug:structure": "find . -type d -maxdepth 3 | sort",
    "build": "npm install && npm run debug:structure && npm run build:client && npm run copy:client && npm run setup:server && npm run build:server"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.8",
    "nodemon": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "fs-extra": "^11.1.1"
  }
}
